function SuccessMessage(text) {
    Toast.fire({
        icon: 'success',
        title: 'Hurray!',
        text: text,
    })
}

function ErrorMessage( text) {
    Toast.fire({
        icon: 'error',
        title: 'oops!',
        text: text,
    })
}






